package cards;

import java.awt.Image;
import java.io.Serializable;

@SuppressWarnings("serial")
public class Card implements Serializable{

	private int power;
	private int suit;
	public Image image;

	public Card(int power, int suit, Image img){
		this.power = power;
		this.suit = suit;
		this.image = img;
	}

	public int getPower(){
		return this.power;
	}

	public int getSuit(){
		return this.suit;
	}
}
